<script>
 jQuery(function() {
    jQuery(".sa_on_off").on('click', function() {
       var event = jQuery.Event("run");
       event.turn_on = function() {
          console.log('ligado!');
       };
       event.turn_off = function(obj) {
          console.log('desligado!' + obj.attr("id"));
       };
       jQuery(this).trigger(event);
    });
 });
</script>
